// Computa
.computa 
	width: 100%
	height 500px
	background: gray
	@media screen and (max-width: 768px)
		width: 768px
	@media screen and (max-width: 414px)
		width: 414px
		height 300px
	.carousel
		@media screen and (max-width: 414px)
			height 300px
		ol 
			li
				width: 10px
		.carousel-inner
			.carousel-item
				@media screen and (max-width: 414px)
					height 300px
				img
					width: 100%
					height 500px
					@media screen and (max-width: 414px)
						width: 100%
						height  300px
				.card-img-overlay
					a
						text-decoration none
						color white
						h1.hesam1
							margin-top: 10%
							transition 2s
							&:hover
								font-size: 60px
								color #b22af1
							@media screen and (max-width: 414px)
								display none	
						h1.hesam2
							width: 40%
							margin-left: 30%
							font-weight: bold
							border 2px solid white
							transition 0.1s
							&:hover
								border 2px solid red
							@media screen and (max-width: 414px)
								width: 80%
								margin-left: 10%
					button 
						width: 10%
						margin-left: 45%
						font-weight: bold
						background: white
						transition: 2s
						@media screen and (max-width: 1024px)
							width: 20%
							margin-left: 40%
						@media screen and (max-width: 414px)
							width: 30%
							margin-left: 35%
							display none
						&:hover
							background: #3d3333
							color white
//Ket thuc Computa
// kohi
.kohi
	width: 100%
	height:700px
	margin-top: 1%
	margin-bottom: 3%
	@media screen and (max-width: 1024px)
		width: 90%
	@media screen and (max-width: 768px)
		width: 768px
	@media screen and (max-width: 414px)
		display none
	.container
		@media screen and (max-width: 1024px)
			padding-left: 6%
		@media screen and (max-width: 768px)
			padding-left: 25%
		a
			display: inline-block
			-webkit-transition:all .4s ease-in-out;
			transition:all .4s ease-in-out
			&:hover img
				-ms-transform:scale(1.2)
				-webkit-transform:scale(1.2)
				transform:scale(1.2)
			&:hover .kohi1
				background: #737373
				h2,h5,button
					color white
				hr 
					background: white
			.hovereffect
				width:95%
				height:350px
				overflow:hidden
				background-color:rgba(0,0,0,0.5)
				-webkit-transition:all .4s ease-in-out
				transition:all .4s ease-in-out
				@media screen and (max-width: 1024px)
					width: 80%
				&:hover img 
					-ms-transform:scale(1.2)
					-webkit-transform:scale(1.2)
					transform:scale(1.2)
			.hovereffect img 
				display:block;
				position:relative;
				-webkit-transition:all .4s linear
				transition:all .4s linear
			.kohi1
				float left
				width: 95%
				height 350px
				@media screen and (max-width: 1024px)
					width: 80%
				&:hover
					background: #737373
					h2,h5,button
						color white
					hr 
						background: white
				h2
					margin-top: 30%
					color black
					font-weight: bold
				hr
					width: 20%
					background: black
				h5
					color black
				button 
					margin-top: 3%
					width: 40%
					margin-left: 30%
					background: none
					color black
					border 2px solid black
					font-weight: bold
					&:hover
						background: black
						color white
						border 2px solid white
